{
  "name": "peibin/laravel-server",
  "description": "Peibin server component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "laravel",
    "server"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^9.0",
    "peibin/laravel-contracts": "^1.0",
    "peibin/laravel-utils": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Server\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeibinLaravel\\Server\\ServerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
